<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0 http://maven.apache.org/xsd/archetype-descriptor-1.0.0.xsd"
					  xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0"
					  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<requiredProperties>
		<requiredProperty key="projectPrettyName" />
		<requiredProperty key="parentApplication" />
		<requiredProperty key="repositoryName" />
		<requiredProperty key="gitignore">
			<defaultValue>.gitignore</defaultValue>
		</requiredProperty>
	</requiredProperties>
	<fileSets>
		<fileSet filtered="true" encoding="UTF-8">
			<directory>src/license</directory>
			<includes>
				<include>**/*.txt</include>
			</includes>
		</fileSet>
		<fileSet encoding="UTF-8">
			<directory></directory>
			<includes>
				<include>__gitignore__</include>
			</includes>
		</fileSet>
	</fileSets>
	<modules>
		<module id="${rootArtifactId}-jpa" dir="__rootArtifactId__-jpa" name="${rootArtifactId}-jpa">
			<fileSets>
				<fileSet filtered="true" encoding="UTF-8">
					<directory>src/main/java/__packageInPathFormat__</directory>
				</fileSet>
				<fileSet filtered="true" encoding="UTF-8">
					<directory>src/test/java/__packageInPathFormat__</directory>
				</fileSet>
				<fileSet filtered="true" encoding="UTF-8">
					<directory>src/main/resources</directory>
					<includes>
						<include>**/*</include>
					</includes>
				</fileSet>
				<fileSet filtered="true" encoding="UTF-8">
					<directory>src/test/resources</directory>
					<includes>
						<include>**/*</include>
					</includes>
				</fileSet>
				<fileSet encoding="UTF-8">
					<directory></directory>
					<includes>
						<include>__gitignore__</include>
					</includes>
				</fileSet>
			</fileSets>
		</module>
		<module id="${rootArtifactId}-core" dir="__rootArtifactId__-core" name="${rootArtifactId}-core">
			<fileSets>
				<fileSet filtered="true" encoding="UTF-8">
					<directory>src/main/java/__packageInPathFormat__</directory>
				</fileSet>
				<fileSet filtered="true" encoding="UTF-8">
					<directory>src/test/java/__packageInPathFormat__</directory>
				</fileSet>
				<fileSet filtered="true" encoding="UTF-8">
					<directory>src/main/resources</directory>
					<includes>
						<include>**/*</include>
					</includes>
				</fileSet>
				<fileSet filtered="true" encoding="UTF-8">
					<directory>src/test/resources</directory>
					<includes>
						<include>**/*</include>
					</includes>
				</fileSet>
				<fileSet encoding="UTF-8">
					<directory></directory>
					<includes>
						<include>__gitignore__</include>
					</includes>
				</fileSet>
			</fileSets>
		</module>
		<module id="${rootArtifactId}-boot" dir="__rootArtifactId__-boot" name="${rootArtifactId}-boot">
			<fileSets>
				<fileSet filtered="true" encoding="UTF-8">
					<directory>src/main/java/__packageInPathFormat__</directory>
					<includes>
						<include>**/*</include>
					</includes>
				</fileSet>
				<fileSet filtered="true" encoding="UTF-8">
					<directory>src/test/java/__packageInPathFormat__</directory>
					<includes>
						<include>**/*</include>
					</includes>
				</fileSet>
				<fileSet filtered="true" encoding="UTF-8">
					<directory>src/main/resources</directory>
					<includes>
						<include>**/*</include>
					</includes>
				</fileSet>
				<fileSet filtered="true" encoding="UTF-8">
					<directory>src/test/resources</directory>
					<includes>
						<include>**/*</include>
					</includes>
				</fileSet>
				<fileSet encoding="UTF-8">
					<directory></directory>
					<includes>
						<include>__gitignore__</include>
					</includes>
				</fileSet>
			</fileSets>
		</module>
	</modules>
</archetype-descriptor>